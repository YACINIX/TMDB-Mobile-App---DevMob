{
  "expo": {
    "name": "Cine Passion",
    "slug": "cine-passion",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",

    "icon": "./assets/icon.png",

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0B0F"
    },

    "assetBundlePatterns": ["**/*"],

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B0B0F"
      }
    },

    "ios": {
      "supportsTablet": true
    },

    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
